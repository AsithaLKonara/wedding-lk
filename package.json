  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "deploy:prod": "./scripts/quick-deploy.sh --prod",
    "deploy:production": "./scripts/deploy-production.sh",
    "deploy:quick": "./scripts/quick-deploy.sh",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "vercel:login": "vercel login",
    "vercel:link": "vercel link",
    "vercel:logs": "vercel logs",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "bundle-analyzer": "npm run build && npx @next/bundle-analyzer",
    "cache-clear": "rm -rf .next && rm -rf node_modules/.cache",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --runInBand",
    "test:e2e": "playwright test",
    "test:api": "jest tests/api --runInBand",
    "test:coverage": "jest --coverage",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:generate": "node scripts/generate-tests.js",
    "test:generate-advanced": "node scripts/cursor-ai-integration.js",
    "test:optimize": "node scripts/cursor-ai-integration.js --optimize-only",
    "test:analyze": "node scripts/cursor-ai-integration.js --analyze-only"
  },
