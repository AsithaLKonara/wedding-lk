{
  "name": "weddinglk-next",
  "version": "1.0.0",
  "description": "A comprehensive wedding planning platform - Final Year University Project",
  "author": "Asitha Lakmal <asitha.lakmal@student.university.edu>",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/asithalakmal/weddinglk-next.git"
  },
  "keywords": [
    "wedding",
    "planning",
    "platform",
    "nextjs",
    "react",
    "typescript",
    "university-project"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "seed": "node scripts/simple-comprehensive-seeder.js",
    "seed:reset": "node scripts/comprehensive-seeder.js --reset",
    "seed:atlas:comprehensive": "node scripts/comprehensive-atlas-seeder.js",
    "seed:atlas:simple": "node scripts/simple-atlas-seeder.js",
    "seed:relational": "node scripts/comprehensive-relational-seeder.js",
    "deploy:prod": "./scripts/quick-deploy.sh --prod",
    "deploy:production": "./scripts/deploy-production.sh",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "seed:old": "node scripts/seed-mongodb-atlas.js",
    "seed:atlas": "node scripts/seed-mongodb-atlas.js",
    "test:atlas": "node scripts/test-mongodb-atlas.js",
    "test:simple": "echo 'No tests configured yet'",
    "test": "vitest",
    "test:unit": "jest --testPathPatterns=tests/unit",
    "test:integration": "jest --testPathPatterns=tests/integration",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test:watch": "jest --watch",
    "test:ui": "vitest --ui",
    "test:all": "npm run test && npm run test:e2e",
    "test:ci": "npm run test:unit -- --ci && npm run test:integration -- --ci",
    "test:performance": "k6 run tests/performance/booking-load.js",
    "test:smoke": "playwright test --grep @smoke",
    "test:security": "npm audit && snyk test",
    "test:production": "playwright test tests/e2e/production-test.spec.ts",
    "test:production-api": "playwright test tests/api/production-api.test.ts",
    "test:production-load": "k6 run tests/load/production-load-test.js",
    "test:production-all": "npm run test:production && npm run test:production-api && npm run test:production-load",
    "docker:build": "./scripts/docker-build.sh",
    "docker:start": "./scripts/docker-manage.sh start",
    "docker:stop": "./scripts/docker-manage.sh stop",
    "docker:restart": "./scripts/docker-manage.sh restart",
    "docker:status": "./scripts/docker-manage.sh status",
    "docker:logs": "./scripts/docker-manage.sh logs",
    "docker:clean": "./scripts/docker-manage.sh clean",
    "docker:seed": "./scripts/docker-manage.sh seed",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "performance": "node scripts/monitor-performance.mjs",
    "bundle-analyzer": "npm run build && npx @next/bundle-analyzer",
    "cache-clear": "rm -rf .next && rm -rf node_modules/.cache",
    "install:all": "npm install && npm run install:test-deps",
    "install:test-deps": "npm install --save-dev vitest @vitest/ui playwright @playwright/test k6 artillery snyk",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:accessibility": "pa11y-ci",
    "test:regression": "playwright test --grep @regression",
    "test:load": "k6 run tests/performance/booking-load.js",
    "test:stress": "k6 run tests/performance/stress-test.js",
    "test:spike": "k6 run tests/performance/spike-test.js",
    "test:volume": "k6 run tests/performance/volume-test.js",
    "test:endurance": "k6 run tests/performance/endurance-test.js",
    "test:report": "allure generate allure-results --clean && allure open allure-report",
    "test:report:generate": "allure generate allure-results --clean",
    "test:report:open": "allure open allure-report",
    "test:db:setup": "node tests/utils/setup-test-db.js",
    "test:db:teardown": "node tests/utils/teardown-test-db.js",
    "test:mock:start": "msw init public/ --save",
    "test:mock:stop": "msw stop",
    "test:performance:monitor": "node scripts/monitor-performance.mjs",
    "test:performance:analyze": "node scripts/analyze-performance.mjs",
    "test:security:scan": "npm audit --audit-level=moderate && snyk test",
    "test:security:fix": "npm audit fix",
    "test:accessibility:pa11y": "pa11y-ci",
    "test:accessibility:axe": "axe-core --dir tests/accessibility",
    "test:accessibility:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "test:accessibility:wave": "wave --input=http://localhost:3000 --output=./wave-report.json",
    "test:accessibility:all": "npm run test:accessibility:pa11y && npm run test:accessibility:axe && npm run test:accessibility:lighthouse",
    "test:api:contract": "jest --testPathPattern=tests/contract",
    "test:api:load": "artillery run tests/performance/api-load.yml",
    "test:api:stress": "artillery run tests/performance/api-stress.yml",
    "test:api:spike": "artillery run tests/performance/api-spike.yml",
    "test:api:volume": "artillery run tests/performance/api-volume.yml",
    "test:api:endurance": "artillery run tests/performance/api-endurance.yml",
    "test:api:all": "npm run test:api:load && npm run test:api:stress && npm run test:api:spike && npm run test:api:volume && npm run test:api:endurance",
    "test:visual:regression": "playwright test --grep @visual",
    "test:visual:baseline": "playwright test --grep @visual --update-snapshots",
    "test:visual:compare": "playwright test --grep @visual --reporter=html",
    "test:visual:all": "npm run test:visual:regression && npm run test:visual:baseline && npm run test:visual:compare",
    "test:mobile:responsive": "playwright test --grep @mobile",
    "test:mobile:performance": "lighthouse http://localhost:3000 --output=json --output-path=./mobile-lighthouse-report.json --only-categories=performance",
    "test:mobile:all": "npm run test:mobile:responsive && npm run test:mobile:performance",
    "test:seo:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./seo-lighthouse-report.json --only-categories=seo",
    "test:seo:axe": "axe-core --dir tests/seo",
    "test:seo:all": "npm run test:seo:lighthouse && npm run test:seo:axe",
    "test:reporting:allure": "allure generate allure-results --clean",
    "test:reporting:junit": "jest --reporter=junit --outputDirectory=./junit-reports",
    "test:reporting:coverage": "jest --coverage --coverageReporters=html --coverageReporters=lcov --coverageReporters=text",
    "test:reporting:performance": "k6 run tests/performance/booking-load.js --out json=./k6-results.json",
    "test:reporting:all": "npm run test:reporting:allure && npm run test:reporting:junit && npm run test:reporting:coverage && npm run test:reporting:performance",
    "test:cleanup": "rm -rf coverage/ allure-report/ junit-reports/ k6-results.json lighthouse-report.json mobile-lighthouse-report.json seo-lighthouse-report.json wave-report.json",
    "test:reset": "npm run test:cleanup && npm run test:db:teardown && npm run test:mock:stop",
    "test:full": "npm run test:all && npm run test:performance && npm run test:security && npm run test:accessibility:all && npm run test:api:all && npm run test:visual:all && npm run test:mobile:all && npm run test:seo:all && npm run test:reporting:all",
    "test:crud": "node scripts/test-crud-operations.js",
    "test:api:crud": "npm run test:crud",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:quick": "./scripts/quick-deploy.sh",
    "vercel:login": "vercel login",
    "vercel:link": "vercel link",
    "vercel:logs": "vercel logs"
  },
