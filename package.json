  },
  "keywords": [
    "wedding",
    "planning",
    "platform",
    "nextjs",
    "react",
    "typescript",
    "university-project"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:comprehensive": "node scripts/comprehensive-test.mjs",
    "test:e2e": "npm run test:comprehensive",
    "test:frontend": "node scripts/test-frontend-integration.mjs",
    "test:404": "node scripts/check-404-errors.mjs",
    "test:all": "node scripts/run-all-tests.mjs",
    "fix:api": "node scripts/fix-api-endpoints.mjs",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "seed": "node scripts/seed-database.js",
    "seed:comprehensive": "node scripts/seed-comprehensive.mjs",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "performance": "node scripts/monitor-performance.mjs",
    "bundle-analyzer": "npm run build && npx @next/bundle-analyzer",
    "cache-clear": "rm -rf .next && rm -rf node_modules/.cache",
    "socket:dev": "node server/socket-server.js",
    "socket:start": "NODE_ENV=production node server/socket-server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run socket:dev\"",
    "start:full": "concurrently \"npm run start\" \"npm run socket:start\""
  },
